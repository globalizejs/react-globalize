{
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "extends": [
    "defaults"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "class-property"
  ],
  "rules": {
    "indent": 2,
    "no-console": 1,
    "no-trailing-spaces": 2,
    "no-unused-vars": [0, {"ignore": ["React"]}],
    "quotes": [2, "double"],
    "semi": 1
  }
}
